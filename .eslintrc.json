{
	"extends": [
		"eslint:recommended",
		"next/core-web-vitals",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"simple-import-sort",
		"prettier"
	],
	"rules": {
		"prettier/prettier": [
			"error"
		],
		"jsx-a11y/anchor-is-valid": [
			"error"
		],
		"react/display-name": "off",
		"simple-import-sort/imports": "warn",
		"simple-import-sort/exports": "warn"
	}
}